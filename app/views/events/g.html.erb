                          <%= form_tag "/bids", method: :post do %>
                          <div>
                            <%= label_tag :bid, "place your bid "   %>
                            <%= text_field_tag :bid, "", class: "form-control" %>
                          </div>
                          <div>
                            <% label_tag :event %>
                            <%= hidden_field_tag :event_id, params[:id] %>
                          </div>
                          <div>
                            <%= hidden_field_tag :lowest_price, @low["lowest_price"] %>
                          </div>
                          <div>
                            <%= hidden_field_tag :event_title, @events["title"] %>
                          </div>
                          <br>
                          <div>
                            <%= submit_tag "Submit", class: "btn_1" %>
                          </div>
                          <% end %>




                        </div>
                      </div>

                      <!-- end widget -->


                    </li>
